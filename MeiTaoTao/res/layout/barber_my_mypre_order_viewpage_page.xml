<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/body_backcolor"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:orientation="horizontal" >

        <TextView
            style="@style/left_text_style2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:paddingLeft="23dp"
            android:text="@string/finish_num" />

        <TextView
            android:id="@+id/tv_num"
            style="@style/left_text_style2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="12"
            android:textColor="@color/text_color5" />

        <TextView
            style="@style/left_text_style2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/num" />

        <TextView
            style="@style/left_text_style2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="20dp"
            android:paddingLeft="17dp"
            android:text="@string/finish_account" />

        <TextView
            android:id="@+id/tv_moneyNum"
            style="@style/left_text_style2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="12000"
            android:textColor="@color/text_color5" />

        <TextView
            style="@style/left_text_style2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/yuan" />
    </LinearLayout>

    <TextView
        android:id="@+id/text12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingBottom="10dp"
        android:paddingLeft="23dp"
        android:paddingRight="17dp"
        android:text="@string/hint_alredy_schedule"
        android:textColor="@color/hint_textcolor"
        android:textSize="12sp" />
    <!-- 这个作为Listview的item -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/signup_location_input_bg"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center_vertical"
                android:src="@drawable/my_booklist_ico_time_cancel" />

            <TextView
                style="@style/left_text_style2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="今天 08:00" />
        </LinearLayout>

        <include layout="@layout/line_horizontal" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <TextView
                style="@style/left_text_style2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/business_type" />

            <TextView
                style="@style/blue_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="等离子烫发" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <TextView
                style="@style/left_text_style2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/appointment_location" />

            <TextView
                style="@style/blue_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="深圳市罗湖区##理发店" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <TextView
                style="@style/left_text_style2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/price" />

            <TextView
                style="@style/blue_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="100元" />
        </LinearLayout>

        <include layout="@layout/line_horizontal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical|right"
            android:paddingRight="15dp"
            android:text="已取消"
            android:textColor="@color/titile_barcolor"
            android:textSize="@dimen/text_size5" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/signup_location_input_bg"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center_vertical"
                android:src="@drawable/my_booklist_ico_time_wait" />

            <TextView
                style="@style/left_text_style2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="明天 08:00" />
        </LinearLayout>

        <include layout="@layout/line_horizontal" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <TextView
                style="@style/left_text_style2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/business_type" />

            <TextView
                style="@style/blue_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="等离子烫发" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <TextView
                style="@style/left_text_style2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/appointment_location" />

            <TextView
                style="@style/blue_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="深圳市罗湖区##理发店" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <TextView
                style="@style/left_text_style2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/price" />

            <TextView
                style="@style/blue_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="100元" />
        </LinearLayout>

        <include layout="@layout/line_horizontal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical|right"
            android:paddingRight="15dp"
            android:text="待用户付款"
            android:textColor="@color/text_color7"
            android:textSize="@dimen/text_size5" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/signup_location_input_bg"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center_vertical"
                android:src="@drawable/my_booklist_ico_time_pay" />

            <TextView
                style="@style/left_text_style2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="2011-11-11 08:00" />
        </LinearLayout>

        <include layout="@layout/line_horizontal" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <TextView
                style="@style/left_text_style2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/business_type" />

            <TextView
                style="@style/blue_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="等离子烫发" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <TextView
                style="@style/left_text_style2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/appointment_location" />

            <TextView
                style="@style/blue_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="深圳市罗湖区##理发店" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginLeft="18dp"
            android:orientation="horizontal" >

            <TextView
                style="@style/left_text_style2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/price" />

            <TextView
                style="@style/blue_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="100元" />
        </LinearLayout>

        <include layout="@layout/line_horizontal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical|right"
            android:paddingRight="15dp"
            android:text="已取消"
            android:textColor="@color/text_color5"
            android:textSize="@dimen/text_size5" />
    </LinearLayout>

    <ListView
        android:id="@+id/listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:dividerHeight="10dp" >
    </ListView>

</LinearLayout>